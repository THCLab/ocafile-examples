--name=patient-bmi
# from patient.ocafile
FROM refn:patient
ADD ATTRIBUTE wgt=Numeric hgt=Numeric

# Add meta data
ADD Overlay Meta
  language="engs"
  name="Patient BMI"
  description="Standard 1 Patient BMI"

# Add character encoding
ADD Overlay CHARACTER_ENCODING
  attribute_character_encodings
    wgt="utf-8"
    hgt="utf-8"

