ADD Overlay META
  language="es"
  description="Tarjeta de identificación estudiantil"
  name="Tarjeta de estudiante"

ADD Overlay META
  language="de"
  description="Identifikationskarte für Studierende"
  name="Studentenausweis"

ADD Overlay CHARACTER_ENCODING
  attribute_character_encoding
    number="utf-8"
    first_name="utf-8"
    last_name="utf-8"
    birth_date="utf-8"
    issue_date="utf-8"
    expiry_date="utf-8"
    issuer="utf-8"
    id="utf-8"

