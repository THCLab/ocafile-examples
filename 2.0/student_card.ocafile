--name=student-card
ADD ATTRIBUTE number=Text first_name=Text last_name=Text birth_date=DateTime issue_date=DateTime expiry_date=DateTime issuer=Text id=Text

ADD Overlay META
  language="en"
  description="Student Identification Card"
  name="Student Card"

ADD Overlay META
  language="pl"
  description="Karta identyfikacyjna studenta"
  name="Karta Studenta"

ADD Overlay META
  language="fr"
